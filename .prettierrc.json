{
    "semi": true,
    "tabWidth": 4,
    "printWidth": 100,
    "singleQuote": true,
    "jsxBracketSameLine": true,
    "trailingComma": "all",
    "importOrder": [
        "^react$|^next.*$",
        "<THIRD_PARTY_MODULES>",
        "^api/(.*)$",
        "^utils(.*)$",
        "^components/(.*)$",
        "^[./]"
    ],
    "importOrderSeparation": true,
    "importOrderSortSpecifiers": true,
    "importOrderCaseInsensitive": true
}
